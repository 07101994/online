msgid "Ctrl + Backspace"
msgstr "Ctrl + Hopa (Backspace)"

#: dist/loleaflet-help.html+div.div.table.tr.td:96
msgid "Delete to end of word"
msgstr "Eyða til enda orðs"

#: dist/loleaflet-help.html+div.div.table.tr.td:96
